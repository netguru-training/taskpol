.col-md-6.col-md-offset-1
  .row
    %h2 Projects
    = link_to "New project", new_project_path
    - unless @projects.empty?
      %table.table.table-striped
        %tbody
          - @projects.each do |p|
            %tr
              %td= p.id
              %td= link_to p.title, project_path(p)
              %td= p.author.name

.col-md-4
  .row
    %h2 News feed
    %br
    = render 'shared/activities'
