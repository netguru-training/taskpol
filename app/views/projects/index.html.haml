- unless @projects.empty?
  %table.table.table-striped
    %thead
      %tr
        %th #
        %th Name
        %th Author
    %tbody
      - @projects.each do |p|
        %tr
          %td= p.id
          %td= link_to p.title, project_path(p)
          %td= p.author.firstname + " " + p.author.lastname
